{% if block.settings.name != blank %}
  {% if localization.country.iso_code == 'AU' %}
    {% if block.settings.url != blank -%}
      <a href="{{ block.settings.url }}">
    {%- endif %}
    {{ block.settings.name }}
    {% if block.settings.url != blank %}</a>{% endif %}
  {% endif %}
{% endif %}

{% schema %}
{
  "name":"Indicator For AUS",
  "settings":[
      {
        "type": "text",
        "id":"name",
        "label": "Name"
      },
     {
            "id": "url",
            "label": "URL",
            "type": "url"
          }
  ],
  "presets": [
    {
         "name": "Indicator For AUS"
    }
  ]
}
{% endschema %}

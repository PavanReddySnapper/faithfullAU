  {
    "name": "Ecommerce",
    "settings": [
      {
        "type": "checkbox",
        "id": "enable_ecommerce",
        "label": "Enable Ecommerce",
        "default": true
      }
    ]
  },
  {
    "name": "Search",
    "settings": [
      {
        "type": "header",
        "content": "Search settings"
      },
      {
        "type": "checkbox",
        "id": "enable_instant_search",
        "label": "Enable Instant Search",
        "default": true
      },
      {
        "type": "header",
        "content": "Instant search settings",
        "info": "Select which search results you want to display for quick search, products will always be displayed."
      },
      {
        "type": "checkbox",
        "id": "search_display_collections",
        "label": "Collections",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "search_display_pages",
        "label": "Pages",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "search_display_articles",
        "label": "Articles",
        "default": true
      },
      {
        "type": "range",
        "id": "search_header_display_results",
        "label": "Max results per category",
        "min": 1,
        "max": 10,
        "default": 4
      },
      {
        "type": "select",
        "id": "search_unavailable_products",
        "label": "Display unavailable products",
        "options": [
          {
            "label": "Show",
            "value": "show"
          },
          {
            "label": "Hide",
            "value": "hide"
          },
          {
            "label": "Display last",
            "value": "last"
          }
        ],
        "default": "last"
      },
      {
        "type": "header",
        "content": "Search Fields",
        "info": "For the best search experience, you should use as few fields as possible."
      },
      {
        "type": "checkbox",
        "id": "search_author",
        "label": "Author",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "search_body",
        "label": "Body",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "search_product_type",
        "label": "Product Type",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "search_tag",
        "label": "Tag",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "search_title",
        "label": "Title",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "search_variants_barcode",
        "label": "Variants Barcode",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "search_variants_sku",
        "label": "Variants SKU",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "search_variants_title",
        "label": "Variants Title",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "search_vendor",
        "label": "Vendor",
        "default": true
      },
      {
        "type": "header",
        "content": "Search Suggestions"
      },
      {
        "type": "checkbox",
        "id": "show_custom_search_suggestions",
        "label": "Show custom search suggestions",
        "default": true
      },
      {
        "type": "text",
        "id": "search_suggestions_terms",
        "label": "Suggestion Keyword List",
        "info": "Comma-separated list of search terms eg. Dresses, Skirts",
        "default": "Tops, Linen, Maxi Dresses, Sets, Midi Dresses, Swim, Sale"
      },
      {
        "type": "link_list",
        "id": "search_suggestions_links",
        "label": "Suggestions Links"
      },
      {
        "type": "collection",
        "id": "search_suggestions_collection",
        "label": "Suggested Products Collection",
        "info": "Select a collection to show in search suggestions. A maximum of 9 products will be shown."
      }
    ]
  },
  {
    "name": "Regionalisation",
    "settings": [
      {
        "type": "header",
        "content": "Enable Regionalisation Features"
      },
      {
        "type": "checkbox",
        "id": "enable_hreflangs",
        "label": "Enable hreflangs",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "enable_region_selector",
        "label": "Enable region selector",
        "default": false,
        "info": "Enable manual region selector in site footer. Must also be added in Footer settings."
      },
      {
        "type": "checkbox",
        "id": "enable_autoredirects",
        "label": "Enable auto-redirects",
        "default": false
      },
      {
        "type": "text",
        "id": "prevent_redirect_param",
        "label": "Prevent redirect URL parameter",
        "placeholder": "currency",
        "info": "OPTIONAL: Prevent auto-redirects if URL contains this query parameter. Can be useful to resolve Google Ads issues."
      },
      {
        "type": "header",
        "content": "Region Data"
      },
      {
        "type": "paragraph",
        "content": "Region data entered below should be duplicated across all stores. Enter data for every region, including this one."
      },
      {
        "type": "header",
        "content": "Region 1"
      },
      {
        "type": "text",
        "id": "1_region_name",
        "label": "Region 1 Name",
        "info": "Will be shown in region selector."
      },
      {
        "type": "text",
        "id": "1_region_code",
        "label": "Region 1 Code",
        "info": "The internal code for this region e.g. aus or AU or asia-pacific."
      },
      {
        "type": "text",
        "id": "1_region_domain",
        "label": "Region 1 Domain",
        "info": "The full URL of the live store e.g. https://www.example.com.au"
      },
      {
        "type": "textarea",
        "id": "1_region_country_list",
        "label": "Region 1 Country Codes",
        "info": "Add a comma-separated list of country codes (in ISO 3166-1 alpha-2 format) that are assigned to the store. e.g. AU,NZ or US,CA or SG,MY,HK. This is used for hreflangs as well as redirect logic."
      },
      {
        "type": "checkbox",
        "id": "1_region_current",
        "label": "Current region",
        "default": false,
        "info": "Check this box for the current store region being edited."
      },
      {
        "type": "checkbox",
        "id": "1_region_default",
        "label": "Default region",
        "default": false,
        "info": "Check this box for the main/international region store ONLY."

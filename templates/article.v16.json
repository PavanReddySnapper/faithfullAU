        "title_gVy7Wf": {
          "type": "title",
          "settings": {
            "title": "Could you speak to some of your favourite design notes at Villa Cleo? What makes your space so unique?",
            "is_primary_heading": false,
            "heading_style": "h3",
            "pre_heading": "",
            "margin_bottom": 24
          }
        },
        "richtext_iMDT9c": {
          "type": "richtext",
          "settings": {
            "text": "<p>We chose Pierre de Bormes, the signature stone of the region, to cover indoor and outdoor flooring and to surround the pool. We also used it on the guest house walls to blend seamlessly with nature.</p>",
            "font_size": "body-medium",
            "content_max_width": "100%",
            "content_alignment": "flex-start",
            "margin_bottom": 24
          }
        }
      },

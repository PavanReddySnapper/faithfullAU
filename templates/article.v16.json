        "title_AH6XpN": {
          "type": "title",
          "settings": {
            "title": "When it came to designing the space, where did you look for sources of inspiration and direction?",
            "is_primary_heading": false,
            "heading_style": "h3",
            "pre_heading": "",
            "margin_bottom": 24
          }
        },
        "richtext_4LaC33": {
          "type": "richtext",
          "settings": {
            "text": "<p>Casa Malaparte, for its playful stairs. Georgia O’Keeffe’s Ghost Ranch for additional touches. And the simplicity of 1960s provençal houses. </p>",

    "rich_text_UbFWPp": {
      "type": "rich-text",
      "blocks": {
        "title_4TaBAn": {
          "type": "title",
          "settings": {
            "title": "Where to stay",
            "is_primary_heading": false,
            "heading_style": "h3",
            "pre_heading": "",
            "margin_bottom": 24
